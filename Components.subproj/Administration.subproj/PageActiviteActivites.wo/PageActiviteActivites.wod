	value = commentActivite;
	class = "dtTextareaIntMotif";
	disabled = isDisabledTextCommentActivite;
}

TextFieldCaarId: WOTextField {
	numberformat = "0";
	size = 5;
	value = caarId;
}

TextFieldCodeActivite: WOTextField {
	numberformat = "0";
	size = 5;
	value = codeActivite;
	disabled = true;
