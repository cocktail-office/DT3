BtnAnnuler: WOSubmitButton {
	action = annuler;
	class = "bouton";
	value = "Annuler";
}

BtnDoChangeLocal: WOSubmitButton {
	action = doChangeToLocalPref;
	class = "bouton";
	value = "Redefinir";
}

BtnDoChangePere: WOSubmitButton {
	action = doChangeToPerePref;
	class = "bouton";
	value = "Supprimer";
}

BtnOK: WOSubmitButton {
	action = ok;
	class = "bouton";
	value = "OK";
}

CheckAfficherListe: WOCheckBox {
	checked = isActiviteAffichable;
}

ComboSwap: WOPopUpButton {
	item = swapItem;
	list = swapList;
	selection = swapSelected;
	disabled = isDisabledComboSwap;
}

CondHasErrorMessage: WOConditional {
	condition = hasErrorMessage;
}

CondIsLocalPrefDef: WOConditional {
	condition = isLocalPrefDef;
}

CondNotIsLocalPrefDef: WOConditional {
	condition = isLocalPrefDef;
	negate = true;
}

DTErrorBox1: DTErrorBox {
	message = errorMessage;
}

